{
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "react/react-in-jsx-scope": "off",
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "React"
            }
        ]
    },
    "extends": ["sznm/react", "plugin:@next/next/recommended"]
}
